<Application x:Class="WowSuite.Launcher.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Startup="Application_Startup"
             StartupUri="LoginDialog.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Resources/Dictionaries/Buttons.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Resources/Dictionaries/Effects.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Resources/Dictionaries/Scroll.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Resources/Dictionaries/Progressbar.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Resources/Dictionaries/Tooltip.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Dialogs/NotifyIconResources.xaml" />
                <ResourceDictionary />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>